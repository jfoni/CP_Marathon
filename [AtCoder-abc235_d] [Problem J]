#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

void solve()
{
    ll a, N;
    cin >> a >> N;

    const int LIMIT = 1000000;
    vector<int> dist(LIMIT + 5, -1);

    queue<ll> q;
    q.push(1);
    dist[1] = 0;

    while (!q.empty())
    {
        ll x = q.front();
        q.pop();

        int d = dist[x];

        if (x == N)
        {
            cout << d << "\n";
            return;
        }

        ll mul = x * a;
        if (mul <= LIMIT && dist[mul] == -1)
        {
            dist[mul] = d + 1;
            q.push(mul);
        }

        if (x >= 10 && x % 10 != 0)
        {
            string s = to_string(x);
            char last = s.back();
            s.pop_back();
            string t = string(1, last) + s;
            ll rot = stoll(t);

            if (rot <= LIMIT && dist[rot] == -1)
            {
                dist[rot] = d + 1;
                q.push(rot);
            }
        }
    }

    cout << -1 << "\n";
}

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);

    solve();
    return 0;
}
