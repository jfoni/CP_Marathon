#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

void solve(){
    int N, Q;
    cin >> N >> Q;

    vector<ll> A(N);
    for (int i = 0; i < N; i++)
    {
        cin >> A[i];
    }

    unordered_map<ll, vector<int>> pos;
    for (int i = 0; i < N; i++)
    {
        pos[A[i]].push_back(i + 1);
    }

    for (int i = 0; i < Q; i++)
    {
        ll x, k;
        cin >> x >> k;

        if (pos.count(x) == 0 || pos[x].size() < k)
            cout << -1 << "\n";
        else
            cout << pos[x][k - 1] << "\n";
    }
}

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    solve();
    return 0;
}
