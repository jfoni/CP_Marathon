#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

void solve()
{
    int N;
    cin >> N;
    vector<ll> arr(N + 1);
    vector<ll> prefix(N + 1, 0);

    for (int i = 1; i <= N; i++)
    {
        cin >> arr[i];
        prefix[i] = prefix[i - 1] + arr[i];
    }

    ll V;
    cin >> V;

    int Q;
    cin >> Q;
    while (Q--)
    {
        int L, R;
        cin >> L >> R;
        ll sum = prefix[R] - prefix[L - 1];
        ll len = R - L + 1;
        ll avg = sum / len;

        if (avg >= V)
            cout << "Hurray! we made it in time!!\n";
        else
            cout << "Oh! we missed it!!\n";
    }
}

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);

    solve();
    return 0;
}
