#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

void solve(){
    string s1, s2;
    cin >> s1 >> s2;

    int x = 0, y = 0;

    for (int i = 0; i < s1.size(); i++) {
        if (isdigit(s1[i])) {
            x += (s1[i] - '0');
        }
    }

    for (int i = 0; i < s2.size(); i++) {
        if (isdigit(s2[i])) {
            y += (s2[i] - '0');
        }
    }

    int a = x + y;
    int b = x * y;
    int c = abs(x - y);

    vector<int> v = {a, b, c};
    sort(v.begin(), v.end());

    cout << "\"" << v[1] << "\"";
}

int main(){
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    solve();
    return 0;
}
