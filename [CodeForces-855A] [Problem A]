#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

void solve(){

    int n;
    cin >> n;

    map<string, int> m;
    
    for(int i = 0; i < n; i++){
        string s; 
        cin >> s;
        if(m[s] == 0){
            cout << "NO" <<endl;
            m[s] += 1;
        }
        else{
            cout << "YES" <<endl;
        }
    }
}
       
int main(){
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);
     
    solve();
    
    return 0;
}
