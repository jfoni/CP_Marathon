#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

void solve() {
    int N, M;
    cin >> N >> M;

    vector<ll> A(N);
    for (int i = 0; i < N; i++) cin >> A[i];

    vector<ll> B(M);
    for (int i = 0; i < M; i++) cin >> B[i];

    map<ll, int> count;
    for (auto x : A) count[x]++;

    for (auto x : B) {
        if (count[x] > 0) count[x]--;
        else {
            cout << "No" << endl;
            return;
        }
    }

    cout << "Yes" << endl;
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);

    solve();
    return 0;
}
