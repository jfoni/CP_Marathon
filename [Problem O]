#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

void solve(){
    int t;
    cin >> t;
    for(int T = 1; T <= t; T++) {
        int n;
        cin >> n;

        vector<ll> a(n);
        for(int i = 0; i < n; i++) {
            cin >> a[i];
        }

        ll count[5] = {0};
        for(int i = 0; i < n; i++) {
            count[a[i] % 5]++;
        }

        ll ans = 0;

        ans += (count[0] * (count[0] - 1)) / 2;

        ans += count[1] * count[4];

        ans += count[2] * count[3];

        cout << "Case " << T << ": " << ans << "\n";
    }

}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);

    solve();
    return 0;
}
